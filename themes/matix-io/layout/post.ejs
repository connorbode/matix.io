---
layout: layout
---

<%- include('_partials/header.ejs', {scrollLinks: false}) %>
<div class="post">
	<div class="top-wrapper">
		<div id="splash-img" class="splash blur-preload" data-preload-image="<%- page.banner_lg %>" style="background-image: url(<%- page.banner_sm %>)"></div>
		<div class="meta">
			<h1><%- page.title %></h1>
			<div class="date"><%- page.date.format('LL') %></div>
		</div>
	</div>
	<div class="wrapper">
		<div class="content">
			<%- page.content %>
		</div>
	</div>
</div>